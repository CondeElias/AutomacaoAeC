<Window x:Class="AeCBot.Front.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AeCBot.Front"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="794">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="370"/>
            <ColumnDefinition Width="424"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="textBlock" FontSize="36" Height="48" Width="442" Grid.ColumnSpan="2" FontFamily="Bahnschrift SemiBold" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Teste de Automaçã"/><Run Language="pt-br" Text="o "/><Run Text="A&amp;C"/></TextBlock>
        <Button x:Name="button" Content="Iniciar teste" Padding="5" HorizontalAlignment="Center" Margin="0,300,0,0" FontFamily="Javanese Text" FontSize="20" BorderBrush="#FF0032A1" Background="White" Foreground="#FF00339F" Click="button_Click_1" Grid.ColumnSpan="2" VerticalAlignment="Top"/>
        <Image x:Name="image" Margin="264,36,264,308" Source="/logo.png" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
